{
  "name": "remix-pages-context",
  "type": "module",
  "version": "0.1.10",
  "description": "A module to make using Cloudflare Pages environment variables easier with Remix.",
  "main": "./dist/index.cjs",
  "exports": {
    "require": "dist/index.cjs",
    "import": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src --format esm,cjs --clean --no-splitting",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "Remix",
    "Cloudflare",
    "context"
  ],
  "author": "Brandon Pittman",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@remix-run/cloudflare": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tsup": "^6.0.1",
    "typescript": "^4.6.3"
  },
  "types": "./build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonpittman/remix-pages-context.git"
  },
  "bugs": {
    "url": "https://github.com/brandonpittman/remix-pages-context/issues"
  },
  "homepage": "https://github.com/brandonpittman/remix-pages-context#readme",
  "peerDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@remix-run/cloudflare": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
